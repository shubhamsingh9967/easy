package pattern;

public class Largestelementinarray {

	public static void main(String[] args) {
		int []a= {10,20,30,1000,10};
		int large=Integer.MIN_VALUE;
		for (int i = 0; i < a.length; i++) {
			if(a[i]>large) {
				large=a[i];
			}
		}
			System.out.println("largest number is:"+large);	

	}

}
