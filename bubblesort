package pattern;

import java.util.Arrays;

public class bubblesort {

	public static void main(String[] args) {
		int a[]= {18,17,16,4,4,50,24,8};
		
		for (int i = 0; i < a.length; i++) {
			int min=a[i];
			for (int j = i+1; j < a.length; j++) {
				
				if(a[i]>a[j]) {
					int temp=a[i];
					a[i]=a[j];
					a[j]=temp;
					
					
				}
				
				
			}
		}
	
System.out.println(Arrays.toString(a));
	}

}
