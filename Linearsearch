package pattern;

public class LinearSearch {

	public static void main(String[] args) {
		int []a= {10,20,30,40,50,8,1,2,3,5};
		int search=4;
		boolean b=false;
		for (int i = 0; i < a.length; i++) {
			
			if(a[i]==search) {
				System.out.println("number is present");
				b=true;
			}
		}
if(b==false) {
	System.out.println("number not present");
}
	}

}
