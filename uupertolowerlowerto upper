
public class lowercaseintouppercase {

	public static void main(String[] args) {
		
		
		String s="SHUbham";
	char	c []=s.toCharArray();
		for(int i=0;i<s.length();i++) {
			if(c[i]>='A' && c[i]<='Z')
			{
				c[i]=   ( char) (c[i]+32);
			}
			else {
				c[i]=   ( char) (c[i]-32);
			}		
			
			}
		s=new String(c);
		System.out.println(s);
		}

	}
