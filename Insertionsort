package pattern;

import java.util.Arrays;

public class Insertionsort {

	public static void main(String[] args) {
	int a[]= {10,20,30,40,50,1,2,3};
	for (int i = 0; i < a.length; i++) {
		int temp=a[i];
		int j=i;
		while(j>0&&a[j-1]>temp) {
			a[j]=a[j-1];
			j=j-1;
		}
		a[j]=temp;
	}
			
	System.out.println(Arrays.toString(a));
	}

}
